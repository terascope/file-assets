{
    "name": "file-assets-bundle",
    "version": "2.2.1",
    "description": "A set of processors for working with files",
    "repository": "https://github.com/terascope/file-assets.git",
    "author": "Terascope, LLC <info@terascope.io>",
    "license": "MIT",
    "private": true,
    "workspaces": [
        "packages/*",
        "asset"
    ],
    "scripts": {
        "asset:build": "yarn && yarn run build",
        "asset:post-build": "./scripts/copy-links.sh",
        "build": "tsc --build",
        "build:watch": "tsc --build --watch",
        "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
        "lint:fix": "yarn lint --fix",
        "setup": "yarn && yarn build --force",
        "test": "ts-scripts test . --",
        "test:all": "yarn workspaces run test",
        "test:watch": "ts-scripts test --watch . --",
        "test:debug": "ts-scripts test --debug . --",
        "publish:changed": "./scripts/publish.sh"
    },
    "dependencies": {},
    "devDependencies": {
        "@terascope/eslint-config": "^0.7.1",
        "@terascope/file-asset-apis": "^0.6.1",
        "@terascope/job-components": "^0.58.5",
        "@terascope/scripts": "^0.50.6",
        "@types/fs-extra": "^11.0.1",
        "@types/jest": "^29.4.0",
        "@types/json2csv": "^5.0.3",
        "@types/node": "^18.14.2",
        "@types/node-gzip": "^1.1.0",
        "aws-sdk": "^2.1325.0",
        "eslint": "^8.35.0",
        "fs-extra": "^11.1.0",
        "jest": "^29.4.3",
        "jest-extended": "^3.2.4",
        "jest-fixtures": "^0.6.0",
        "lz4-asm": "^0.4.2",
        "node-gzip": "^1.1.2",
        "node-notifier": "^10.0.1",
        "teraslice-test-harness": "^0.28.2",
        "ts-jest": "^29.0.5",
        "typescript": "~4.9.5"
    },
    "engines": {
        "node": ">=14.17.0"
    },
    "terascope": {
        "root": true,
        "testSuite": "minio",
        "tests": {
            "suites": {
                "unit": [],
                "minio": [
                    "minio"
                ]
            }
        },
        "docker": {
            "registries": [
                "terascope/teraslice"
            ]
        },
        "npm": {
            "registry": "https://registry.npmjs.org/"
        }
    }
}
