{
    "name": "@terascope/file-asset-apis",
    "displayName": "File Asset Apis",
    "version": "1.1.2",
    "description": "file reader and sender apis",
    "homepage": "https://github.com/terascope/file-assets",
    "repository": "git@github.com:terascope/file-assets.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "type": "module",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "files": [
        "dist/src/**/*"
    ],
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "build:watch": "yarn build --watch",
        "test": "ts-scripts test . --",
        "test:debug": "ts-scripts test --debug . --",
        "test:watch": "ts-scripts test --watch . --"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "~3.873.0",
        "@smithy/node-http-handler": "~4.1.1",
        "@terascope/utils": "~1.10.2",
        "csvtojson": "~2.0.10",
        "fs-extra": "~11.3.1",
        "json2csv": "5.0.7",
        "lz4-asm": "~0.4.2",
        "node-gzip": "~1.1.2"
    },
    "devDependencies": {
        "@terascope/scripts": "~1.21.4",
        "@types/jest": "~30.0.0",
        "aws-sdk-client-mock": "~4.1.0",
        "jest": "~30.1.2",
        "jest-extended": "~6.0.0",
        "jest-fixtures": "~0.6.0",
        "ts-jest": "~29.4.1"
    },
    "engines": {
        "node": ">=22.0.0",
        "yarn": ">=1.22.19"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true,
        "testSuite": "s3"
    }
}
