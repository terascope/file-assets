    "devDependencies": {
        "@terascope/scripts": "^0.77.3",
        "@types/jest": "^29.5.12",
        "aws-sdk-client-mock": "^4.0.1",
        "jest": "^29.7.0",
        "jest-extended": "^4.0.2",
        "jest-fixtures": "^0.6.0",
        "ts-jest": "^29.1.5"
    },
    "peerDependencies": {},
    "engines": {
        "node": ">=18.0.0",
        "yarn": ">=1.22.19"
    },
