    "author": "Terascope, LLC <info@terascope.io>",
    "license": "MIT",
    "dependencies": {
        "@terascope/utils": "^0.45.5",
        "csvtojson": "^2.0.10",
        "fs-extra": "^11.1.0",
        "json2csv": "5.0.7",
        "lz4-asm": "^0.4.2",
        "node-gzip": "^1.1.2"
    },
    "peerDependencies": {},
    "devDependencies": {
        "@types/jest": "^29.4.0",
        "aws-sdk": "^2.1325.0",
        "jest": "^29.4.3",
        "jest-extended": "^3.2.4",
        "jest-fixtures": "^0.6.0",
        "ts-jest": "^29.0.5"
    },
