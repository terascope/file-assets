{
    "name": "@terascope/file-asset-apis",
    "version": "0.6.1",
    "description": "file reader and sender apis",
    "publishConfig": {
        "access": "public"
    },
    "srcMain": "src/index.ts",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "files": [
        "dist/src/**/*"
    ],
    "scripts": {
